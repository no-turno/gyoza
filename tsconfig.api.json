{
    "compilerOptions": {
        "moduleResolution": "Node",
        "target": "ESNext",
        "module": "ESNext",
        "lib": ["ESNext", "DOM"],
        "jsx": "react-jsx",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "types": [
            "bun-types"
        ],
        "composite": true,
        "emitDeclarationOnly": true
    },
    "include": ["api/**/*.ts", "**/*.d.ts"]
}