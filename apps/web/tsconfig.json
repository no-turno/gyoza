{
  "compilerOptions": {
    "lib": [
      "ESNext",
      "DOM"
    ],
    "target": "ESNext",
    "module": "ESNext",
    "moduleDetection": "force",
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "bundler",
    "verbatimModuleSyntax": true,
    "strict": true,
    "composite": true,
    "baseUrl": "./",
    "paths": {
      "app/*": [
        "src/app/*"
      ],
      "api/*": [
        "src/api/*"
      ]
    },
    "outDir": "../../.moon/cache/types/apps/web"
  },
  "files": [
    "./app.config.ts"
  ],
  "references": [
    {
      "path": "./tsconfig.api.json"
    },
    {
      "path": "./tsconfig.app.json"
    }
  ],
  "include": [
    "**/*"
  ]
}
